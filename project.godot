; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="VivaVocabulary"
run/main_scene="res://login.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/max_fps=240
boot_splash/image="res://sprites/viva-logo.png"
boot_splash/use_filter=false
config/icon="res://sprites/head.png"
boot_splash/minimum_display_time=60

[audio]

general/text_to_speech=true

[autoload]

Firebase="*res://addons/godot-firebase/firebase/firebase.tscn"
GlobalSignals="*res://GlobalSignals.gd"
scene_manager="*res://scene_manager.gd"
PromptList="*res://PromptList.gd"
Global="*res://global.gd"
GameManager="*res://GameManager.gd"

[display]

window/size/viewport_width=2575
window/size/viewport_height=1290
window/size/window_width_override=932
window/size/window_height_override=430
window/energy_saving/keep_screen_on=false
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"
window/vsync/vsync_mode=0
window/ios/allow_high_refresh_rate=false
window/ios/hide_home_indicator=false
window/ios/hide_status_bar=false
window/ios/suppress_ui_gesture=false

[dotnet]

project/assembly_name="VivaVocabulary"

[editor]

movie_writer/disable_vsync=true
movie_writer/fps=20
import/reimport_missing_imported_files=false
movie_writer/size/window_width_override=0
movie_writer/size/window_height_override=0

[editor_plugins]

enabled=PackedStringArray("res://addons/HTTPSSEClient/plugin.cfg", "res://addons/godot-firebase/plugin.cfg", "res://addons/http-sse-client/plugin.cfg")

[filesystem]

import/blender/enabled=false

[importer_defaults]

animation_library={
"animation/fps": 65.0
}

[physics]

common/physics_ticks_per_second=20
common/max_physics_steps_per_frame=1

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
occlusion_culling/use_occlusion_culling=true
